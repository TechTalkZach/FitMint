{
  "expo": {
    "name": "FitMint",
    "slug": "FitMint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "fitmint",
    "githubUrl": "https://github.com/jongan69/FitMint",
    "description": "A Workout App",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00B08C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#00B08C"
      }
    },
    "web": {
      "favicon": "./src/assets/images/favicon.ico",
      "bundler": "metro"
    }
  }
}
